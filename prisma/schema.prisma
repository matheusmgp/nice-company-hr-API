// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Register {
  id         Int      @id @default(autoincrement())
  name       String
  email      String
  cpf        String
  phone      String
  status     Boolean
  assignedAt DateTime @default(now())
  knowledges String

  @@map("registers")
}
